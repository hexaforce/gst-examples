{
  "name": "webrtc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cra-template": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "fmt": "prettier --write \"./**/*.{js,jsx,css,html}\"",
    "build": "react-scripts build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
